<?xml version="1.0" encoding="UTF-8"?>
<odoo>
<data>
    <record id="res_partner_inherit_form_view" model="ir.ui.view">
       <field name="name">res.partner.inherit.form.view</field>
       <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form" />
       <field name="arch" type="xml">

         <xpath expr="//sheet/notebook/page[@name='sales_purchases']/group/group/field[@name='property_account_position_id']" position="before">
            <field name='contact'/>
        </xpath>

        <xpath expr="//field[@name= 'property_account_position_id']" position="attributes">
            <attribute name="invisible">1</attribute>
        </xpath> 

        <xpath expr="//sheet/notebook/page[@name='accounting']/group/group/button[@name='65']" position="before">
                <field name='contact'/>  
        </xpath>

        <xpath expr="//group[@name= 'purchase']" position="attributes">
            <attribute name="invisible">1</attribute>
        </xpath> 
       
       

       </field>
    </record> 
</data>
</odoo>